<h1>Listing movies</h1>

<footer class="footer">

<table>
  <thead>
    <tr>
     <th>Title</th>
 <th>Description</th>
      <th>Cast</th>
      <th>Genre</th>
      <th>Length</th>
      <th>Url</th>
      <th>Poster url</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>




  <tbody>
    <% @movies.each do |movie| %>
      <tr>
        <td><%= movie.title %></td>
        <td><%= movie.description %></td>
        <td><%= movie.cast %></td>
        <td><%= movie.genre %></td>
        <td><%= movie.length %></td>
        <td><%= movie.url %></td>
        <td><%= movie.poster_url %></td>
        <td><%= movie.year %></td>
        <td><%= link_to 'Show', movie %></td>
        <td><%= link_to 'Edit', edit_movie_path(movie) %></td>
        <td><%= link_to 'Destroy', movie, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Movie', new_movie_path %>




